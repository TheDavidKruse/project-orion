<div class="col-md-11">
  <table class="table table-striped table-hover ">
<thead>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Status</th>
  </tr>
</thead>
<tbody>
  <% for (var i = 0; i< todos.length; i++){%>
    <% var {name, description, status} = todos[i]%>
  <tr>
    <td><%= name %></td>
    <td><%= description %></td>
    <td><%= status %></td>
  </tr>
<%}%>
</tbody>
</table>



</div>
