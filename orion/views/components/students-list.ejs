<%var s = [];%>
<% for (var i = 0; i < cohorts.length; i++){ %>
  <h2><%=cohorts[i].cohort_name%></h2>
  <table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
  <% for(var j = 0; j < students.length; j++){ %>
    <% if (students[j].cohorts_id === cohorts[i].id){ %>
      <% s.push(students[j])%>
      <tr>
        <td>
          <%= students[j].first_name +" "+ students[j].last_name %>
        </td>
        <td>
          <%= students[j].email %>
        </td>
        <td>
        <form action="/staff/<%= students[j].id%>/profile" method="get">
          <input type="submit" name="id" value="<%= students[j].id%>">
          </form>
        </td>
      </tr>
    <%}%>
    <%}%>

  </tbody>
  </table>
<%}%>
