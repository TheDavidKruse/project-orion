<div class="col-md-11">
  <div class="row">
    <h3><%= students[0].student_name %></h3>
    <h4><%= students[0].email %></h4>
  </div>
  <div id="profile" class="row">
    <div id="student-pic-background" class="col-md-2">
      <img id="student-picture" src="<%= students[0].photo_url %>" alt="">
    </div>
    <div class="col-md-3"></div>
    <div class="col-md-7">
      <table class="table table-striped table-hover ">
        <thead>
          <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Status</th>
          </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i< students.length; i++){%>
            <% var {name, description, status} = students[i]%>
              <tr>
                <td><%= name %></td>
                <td><%= description %></td>
                <td><%= status %></td>
              </tr>
              <%}%>
        </tbody>
      </table>
    </div>
  </div>

</div>
