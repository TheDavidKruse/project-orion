    <table class="table table-striped table-hover ">


  <thead>
    <tr>
      <th>Student</th>
      <th>Assignment</th>
      <th>Description</th>
      <th>Status</th>
      <th>Due Date</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < staffHome.length; i++){%>
      <% var {st, todo_id, date_due, staff_id, staff_first, staff_last, student_first, student_last, assignment_name, description, completed, } = staffHome[i] %>
    <tr>
      <td><%= student_first %> <%= student_last%></td>
      <td><%= assignment_name %></td>
      <td><%= description %></td>
      <td><% if (!completed){%>
        not complete
        <%} else{%>
          complete
        <%}%>
        </td>
      <td><%= date_due %>
      <%console.log('from staffHome', completed)%>
      <% if (!completed){%>
      <form action="/todos/complete/<%=st%>" method="post">
      <td> <input type="hidden" name="staff_id" value="<%=staff_id%>">
      <button >Mark Complete</button></td>
      </form>
        <%} else { %>
          <form action="/todos/notcomplete/<%=st%>" method="post"> <input type="hidden" name="staff_id" value="<%=staff_id%>">
          <td><button >Not Complete</button></td>
          </form>
          <%}%>
    </tr>
  <%}%>
  </tbody>
  </table>
